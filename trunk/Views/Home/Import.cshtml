@model System.Data.DataTable 
@using System.Data; 

@{
    ViewBag.Title = "Import";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Import</h2>


<h3>The following data have been imported into database:</h3>

<table> 
    <thead> 
    <tr> 
    @foreach (DataColumn col in Model.Columns)     
    {          
        <th>@col.ColumnName</th> 
    }     
    </tr> 
    </thead>         
    <tbody> 
    @foreach (DataRow row in Model.Rows)     
    {         
        <tr> 
        @foreach (DataColumn col in Model.Columns)         
        {            
            @* Login control is sufficient
            if (col.ColumnName == "Patient_Name" || col.ColumnName == "Patient_Tel")
            {
            <td>不可见</td>
            }
            else
            {
            <td>@row[col.ColumnName]</td> 
            }*@            
            <td>@row[col.ColumnName]</td>
        }         
        </tr> 
    }     
    </tbody> 
</table> 


