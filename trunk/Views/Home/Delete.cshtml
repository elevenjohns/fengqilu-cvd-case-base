@model FengQiLu.Models.CVD
@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>删除</legend>

    <div class="display-label">病人号</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Patient_ID)
    </div>

    <div class="display-label">姓名</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Patient_Name)
    </div>

    <div class="display-label">性别</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Patient_Gender)
    </div>

    <div class="display-label">出生年月</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Patient_Birthday)
    </div>
</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("Back to List", "Index")
    </p>
}
